{
  "name": "jord-jd/laravel-offensive-validation-rule",
  "description": "Laravel validation rule that checks if a string is offensive.",
  "type": "library",
  "require": {
    "php": "^7.2 || ^8.0",
    "illuminate/contracts": "^5.5 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0",
    "jord-jd/is_offensive": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "license": "LGPL-3.0-only",
  "authors": [
    {
      "name": "Jordan Hall",
      "email": "jordan@hall05.co.uk"
    }
  ],
  "autoload": {
    "psr-4": {
      "JordJD\\LaravelOffensiveValidationRule\\": "./src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JordJD\\LaravelOffensiveValidationRule\\Tests\\": "./tests/"
    }
  },
  "replace": {
    "divineomega/laravel-offensive-validation-rule": "self.version"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  }
}
